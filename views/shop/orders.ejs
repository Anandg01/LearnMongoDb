<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
          <% if(orders.length>0){ %>
         <ul>
            <% orders.forEach(order=>{%>
             <li class="order-heading">
                <h1>#<%= order._id %></h1>
                <ul>
                    <% order.items.forEach(product=>{%>
                      <li class="order-des"> 
                        <%= product.title %>
                       (<%= product.quantity %>)
                      </li>
                 <% });%>
                </ul>
             </li>
           <% });%>
         </ul>
    
       <% } else { %>
        <h1>Nothing there!</h1>
          <% }%>

        </main>
        <%- include('../includes/end.ejs') %>